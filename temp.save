body {
    font-family: 'Arial', sans-serif;
    line-height: 1.6;
    margin: 0;
    padding: 0;
    background-color: #f4f4f4;
}

header {
    background: #1e1e1e;
    color: #fff;
    padding: 20px 0;
    text-align: center;
}

nav ul {
    list-style: none;
    padding: 0;
}

nav ul li {
    display: inline;
    margin: 0 15px;
}

nav ul li a {
    color: #fff;
    text-decoration: none;
}

#teams {
    padding: 20px;
    text-align: center;
}

.team-list {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(150px, 1fr));
    gap: 15px;
    padding: 20px;
}

.team {
    background: #fff;
    border-radius: 10px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    text-align: center;
    padding: 10px;
    transition: transform 0.2s;
}

.team:hover {
    transform: scale(1.05);
}

.team img {
    max-width: 100%;
    border-radius: 10px;
}

.team h3 {
    color: #333;
    margin: 10px 0 5px;
}

.team p {
    font-size: 14px;
    color: #666;
}

.btn {
    background: #007BFF;
    color: #fff;
    border: none;
    border-radius: 5px;
    padding: 8px 12px;
    cursor: pointer;
    transition: background 0.3s;
}

.btn:hover {
    background: #0056b3;
}

footer {
    text-align: center;
    padding: 20px 0;
    background: #1e1e1e;
    color: #fff;
    position: relative;
    bottom: 0;
    width: 100%;
